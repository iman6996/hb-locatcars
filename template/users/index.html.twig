{% extends "base.html.twig" %}

{% block title %}
Listes des utilisateurs
{% endblock %}

{% block content %}
<h1>Utilisateurs </h1>
<ul>
    {% for user in users %}
    <li>
        <a href="{{env.base_path}}/user/{{user.id}}">
            {{user.lastname}} {{user.firstname}} <hr>
        </a>
    </li>
    {% endfor %}
</ul>
{% endblock %}