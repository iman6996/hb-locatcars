{% extends "base.html.twig" %}

{% block title %}
Listes des utilisateurs
{% endblock %}

{% block content %}
<h1>Utilisateurs </h1>
<ul>
    {% for user in users %}
    <li>
        <a href="{{env.base_path}}/users/{{user.id}}">
            {{user.id}} <br>
            {{user.lastname}} {{user.firstname}} <hr>
        </a>
        
    </li>
    {% endfor %}
</ul>
<a href="{{env.base_path ~ 'users/new/' ~ car.id}}">
<input type="submit" value="Ajouter un utilisateur"></a>
{% endblock %}